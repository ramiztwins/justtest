{
    "inspector": {
        "region": "west-coast"
    },
    "metrics_db": [
        {
            "influxdb": {
                "database_url": "inspector-influxdb",
                "port": 8086,
                "database_name": "inspector",
                "transport_protocol": "http"
            }
        }
    ],
    "targets": [
        {
            "id": "github",
            "name": "Github's pricing page",
            "probers": [
                {
                    "id": "GH",
                    "name": "basic_http_prober",
                    "context": {
                        "url": "https://github.com/pricing",
                        "method": "GET",
                        "parameters": {
                            "param1": "foo",
                            "param2": "bar"
                        },
                        "cookies": {
                            "name": "myname"
                        },
                        "allow_redirects": false,
                        "timeout": "6s"
                    }
                }
            ]
        },
        {
            "id": "google",
            "name": "Google's home page",
